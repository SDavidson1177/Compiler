procedures -> procedure procedures
procedures -> main
procedure -> INT ID LPAREN params RPAREN LBRACE body RETURN expr SEMI RBRACE
main -> INT MAIN LPAREN dcl COMMA dcl RPAREN LBRACE body RETURN expr SEMI RBRACE
body -> 
body -> dcls body
body -> statement body
params ->
params -> paramlist
paramlist -> dcl
paramlist ->dcl COMMA paramlist
type -> INT
type -> INT STAR
// dcls ->
dcls -> dcl BECOMES NUM SEMI
dcls -> dcl BECOMES NULL SEMI
dcl -> type ID
// statements ->
// statements -> statements statement
statement -> lvalue BECOMES expr SEMI
statement -> IF LPAREN test RPAREN LBRACE body RBRACE ELSE LBRACE body RBRACE 
statement -> WHILE LPAREN test RPAREN LBRACE body RBRACE 
statement -> PRINTLN LPAREN expr RPAREN SEMI
statement -> DELETE LBRACK RBRACK expr SEMI
test -> expr EQUALS expr  
test -> expr NEQUALS expr  
test -> expr LT expr 
// test -> expr LE expr  
// test -> expr GE expr  
test -> expr GT expr 
expr -> term 
expr -> expr PLUS term 
expr -> expr MINUS term 
term -> factor 
term -> term STAR factor 
term -> term SLASH factor 
term -> term PCT factor 
factor -> ID  
factor -> NUM  
factor -> NULL  
factor -> LPAREN expr RPAREN  
factor -> AMP lvalue
factor -> STAR factor
factor -> NEW INT LBRACK expr RBRACK
factor -> ID LPAREN RPAREN
factor -> ID LPAREN arglist RPAREN
arglist -> expr
arglist -> expr COMMA arglist
lvalue -> ID  
lvalue -> STAR factor
lvalue -> LPAREN lvalue RPAREN 

